table bigGenePred
"bigGenePred gene models"
   (
   string chrom;       "Reference sequence chromosome or scaffold"
   uint   chromStart;  "Start position in chromosome"
   uint   chromEnd;    "End position in chromosome"
   string name;        "Transcript name"
   uint score;         "Score (0-1000)"
   char[1] strand;     "+ or - for strand"
   uint thickStart;    "Start of where display should be thick (start codon)"
   uint thickEnd;      "End of where display should be thick (stop codon)"
   uint reserved;       "RGB value (use R,G,B string in input file)"
   int blockCount;     "Number of blocks"
   int[blockCount] blockSizes; "Comma separated list of block sizes"
   int[blockCount] chromStarts; "Start positions relative to chromStart"
   string gene_name; "Name of gene"
   string gene_id; "ENSEMBL ID of gene"
   string gene_biotype; "Biotype of gene in GENCODE"
   string gene_biotype_category; "Higher-resolution biotype of gene in GENCODE"
   string transcript_id; "Transcript ID"
   int transcript_length; "Length of transcript"
   float transcript_avg_tpm; "Average TPM of this transcript across replicates of this sample"
   float transcript_percent_isoform; "Percent isoform (pi) value of transcript aggregated across replicates of this sample"
   int transcript_rank; "Rank of transcript within gene"
   string major_transcript; "Whether this transcript is part of the major set in this sample"
   string tss_id; "ID of transcript start site (TSS)"
   string tss_novelty; "Novelty status of TSS relative to GENCODE"
   string ic_id; "ID of intron chain / exon junction chain"
   string ic_novelty; "Novelty status of IC / EC relative to GENCODE"
   string tes_id; "ID of transcript end site (TES)"
   string tes_novelty; "Novelty status of TES relative to GENCODE"
   string sample; "Sample that this track is for"
   string ENCODE_experiment_ids; "Comma-separated list of ENCODE experiment IDs used for this sample"
   )

